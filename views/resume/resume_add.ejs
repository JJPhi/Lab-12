<!DOCTYPE html>
<html>
<head>
    <title>Resume - Add</title>
</head>
<body>
<h1>Add New Resume</h1>
<form action="/resume/insert" method="get">
    <input type="hidden"
           id="account_id"
           name="account_id"
           type="text"
           value="<%= account_id %>"
    />
    Resume Name: <input name="resume_name" type="text" />
    <br />
    <br />
    Skill
    <br />
    <% for(var i=0; skill_result.length > i; i++ ) { %>
    <input type="checkbox" name="skill_id" value="<%=
        skill_result[i].skill_id %>"> <%= skill_result[i].skill_name + ', '
    + skill_result[i].description %>
    <br />
    <% } %>
    <br />
    School
    <br />
    <% for(var i=0; school_result.length > i; i++ ) { %>
    <input type="checkbox" name="school_id" value="<%=
        school_result[i].school_id %>"> <%= school_result[i].school_name %>
    <br />
    <% } %>
    <br />
    Company
    <br />
    <% for(var i=0; company_result.length > i; i++ ) { %>
    <input type="checkbox" name="company_id" value="<%=
        company_result[i].company_id %>"> <%= company_result[i].company_name %>
    <br />
    <% } %>
    <br />
    <input type="submit" />

</form>
</body>
</html>
