<!DOCTYPE html>
<html>
<head>
    <title>Account - View All</title>
</head>
<body>
<h1>Account</h1>

<table border="1">
    <tr><th>Email</th><th>First Name</th><th>Last Name</th><th>Actions</th></tr>
    <% for(var i = 0; i < account.length; i++) { %>
    <tr>
        <td>
            <a href="/account/?account_id=<%= account[i].account_id %>">
                <%= account[i].email %>
            </a>
        </td>
        <td>
            <a href="/account/?account_id=<%= account[i].account_id %>">
                <%= account[i].first_name %>
            </a>
        </td>
        <td>
            <a href="/account/?account_id=<%= account[i].account_id %>">
                <%= account[i].last_name %>
            </a>
        </td>
        <td>
            <a href="/account/edit/?account_id=<%= account[i].account_id %>">
                Edit
            </a>
        </td>
    </tr>

    <% } %>
</table>

</body>
</html>